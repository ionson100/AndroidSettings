<?xml version="1.0" encoding="utf-8"?>
<resources>





    <style name="edit_text_3d">
        <item name="android:background">@drawable/edittext_shape</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textColor">#083503</item>
        <item name="android:layout_margin">2dp</item>
    </style>



    <style name="setting_smail_text">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#8F000009</item>
    </style>

    <style name="settings_value">


        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="value_big" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">30dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">5dp</item>
        <item name="android:paddingLeft">10dp</item>

    </style>


    <style name="buttonSimple">
        <item name="android:textSize">16sp</item>
        <item name="android:background">@drawable/settings_simple_button</item>

        <item name="android:textColor">@color/colors_button_simple</item>
        <item name="android:shadowColor">#05803A</item>

    </style>
    <style name="buttonSettings">
        <item name="android:textSize">16sp</item>
        <item name="android:background">@color/cardview_dark_background</item>

        <item name="android:textColor">@color/colors_button_simple</item>

    </style>

</resources>